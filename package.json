{
  "name": "diff-serving",
  "version": "1.0.0",
  "description": "A toy app demonstrating how to do differential serving step by step with git tags.",
  "main": "server.js",
  "scripts": {
    "clean": "npx rimraf ./dist",
    "build": "npm run clean && NODE_ENV=production npx webpack --progress && npm run copy-images",
    "build:dev": "npm run clean && NODE_ENV=development npx webpack --progress && npm run copy-images",
    "copy-images": "cp -Rf ./src/client/images ./dist/client/images",
    "start": "npm run build && NODE_ENV=production node ./dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malchata/diff-serving.git"
  },
  "keywords": [
    "webpack",
    "code",
    "splitting",
    "javascript"
  ],
  "author": "Jeremy L. Wagner <jeremy.l.wagner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malchata/code-splitting-example/issues"
  },
  "homepage": "https://github.com/malchata/code-splitting-example#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.4",
    "preact": "^8.4.2",
    "preact-render-to-string": "^4.1.0",
    "preact-router": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.4.9",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.15.0",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  }
}
